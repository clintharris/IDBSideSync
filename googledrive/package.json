{
  "name": "@clintharris/idb-sidesync-gdrive",
  "version": "0.1.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/clintharris/idb-sidesync.git"
  },
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "module": "dist/lib-gdrive.esm.js",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "clean": "del dist",
    "build": "npm run tsdx -- build",
    "watch": "npm run tsdx -- watch",
    "pretsdx": "npm run clean",
    "tsdx": "tsdx --format umd --name IDBSideSync.plugins.googledrive",
    "test": "tsdx test",
    "lint": "tsdx lint src",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "size-limit": [
    {
      "path": "dist/lib-gdrive.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/lib-gdrive.esm.js",
      "limit": "10 KB"
    }
  ],
  "peerDependencies": {},
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.9.1",
    "@types/gapi": "0.0.39",
    "@types/gapi.auth2": "0.0.53",
    "@types/gapi.client.drive": "^3.0.13",
    "del-cli": "^3.0.1",
    "husky": "^4.3.6",
    "size-limit": "^4.9.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.0.3",
    "typescript": "^4.1.3"
  }
}
